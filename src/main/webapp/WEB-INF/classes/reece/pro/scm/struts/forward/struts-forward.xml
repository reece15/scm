<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="forward" namespace="/" extends="interceptor">
		<action name="forwardAction_*" method="{1}" class="reece.pro.scm.forward.action.ForwardAction">
			<result name="role">
				WEB-INF/jsp/scm/basedata/role/role.jsp
			</result>
			<result name="left">
				WEB-INF/jsp/scm/frame/left.jsp
			</result>
			<result name="right">
				WEB-INF/jsp/scm/frame/right.jsp
			</result>
			<result name="center">
				WEB-INF/jsp/scm/frame/center.jsp
			</result>
			<result name="main">
				WEB-INF/jsp/scm/frame/mian.jsp
			</result>
			<result name="top">
				WEB-INF/jsp/scm/frame/top.jsp
			</result>
		</action>
	</package>
</struts>
